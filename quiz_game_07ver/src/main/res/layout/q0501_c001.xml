            android:hint="@string/q0501_t105_1"
            android:inputType="text"
            android:textColor="@color/Black"
            tools:ignore="DuplicateSpeakableTextCheck,TouchTargetSizeCheck,TextContrastCheck"
            tools:targetApi="o" />

        <TextView
            android:id="@+id/q0501_t101"
            android:layout_width="@dimen/p080"
            android:layout_height="@dimen/p030"
