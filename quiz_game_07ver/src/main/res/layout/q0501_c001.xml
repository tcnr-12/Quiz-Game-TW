            android:layout_alignParentEnd="true"
            android:layout_marginTop="@dimen/p060"
            android:layout_marginEnd="@dimen/p005"
            android:background="@android:drawable/btn_default"
            android:text="@string/q0501_b107" />

        <Button
            android:id="@+id/q0501_b108"
            android:layout_width="wrap_content"
