            android:hint="@string/q0501_t105"
            android:inputType="text"
            android:minHeight="28dp"
            android:textColor="@color/Black"
            tools:ignore="DuplicateSpeakableTextCheck,TouchTargetSizeCheck,TextContrastCheck"
            tools:targetApi="o" />

        <TextView
