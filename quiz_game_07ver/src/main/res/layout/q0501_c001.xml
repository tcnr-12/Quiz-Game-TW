    <LinearLayout
        android:id="@+id/q0501_ll32"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:visibility="invisible">

        <Button
            android:id="@+id/q0501_b110"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/big_button"
            android:text="@string/q0501_b110"
            android:textAlignment="center"
            android:textColor="@color/big_button_text"
            android:textSize="18sp"
            android:textStyle="bold"
            tools:ignore="TextContrastCheck" />

        <LinearLayout
            android:id="@+id/q0501_ll34"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="gone">

            <TextView
                android:id="@+id/q0501_c001_subtitle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/light_yellow"
                android:text="@string/q0501_c001_subtitle"
                android:textColor="@color/big_button_text"
                android:textSize="18sp"
                tools:ignore="TextContrastCheck" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <Button
                    android:id="@+id/q0501_b111"
                    style="?android:attr/buttonBarButtonStyle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@color/big_button"
                    android:text="@string/q0501_b111"
                    android:textColor="@color/Olive"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <Button
