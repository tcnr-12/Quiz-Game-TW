            android:layout_alignParentEnd="true"
            android:layout_marginStart="@dimen/p000"
            android:layout_marginTop="@dimen/p010"
            android:layout_marginEnd="@dimen/p000"
            android:layout_toEndOf="@+id/q0501_t103"
